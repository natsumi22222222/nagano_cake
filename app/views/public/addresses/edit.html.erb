<%= form_with model: @address, url: admin_adresses_path(@address.id), method: :patch do |f| %>

  <labal>郵便番号（ハイフンなし）</labal>
  <%= f.text_field :postal_code %>
  <labal>住所</labal>
   <%= f.text_field :address %>
  <labal>宛名</labal>
   <%= f.text_field :name %>
   <%= f.submit '変更内容を保存' %>
<% end %>